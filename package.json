{
  "name": "markdowndb",
  "version": "0.1.1",
  "description": "Parse markdown files and store them in an SQL database.",
  "bin": {
    "mddb": "./src/bin/index.js"
  },
  "scripts": {
    "format:check": "prettier --check \"**/*.{js,ts}\"",
    "format:write": "prettier --write \"**/*.{js,ts}\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datopian/markdowndb.git"
  },
  "keywords": [
    "markdown",
    "database",
    "content",
    "obsidian"
  ],
  "author": "Rufus Pollock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datopian/markdowndb/issues"
  },
  "homepage": "https://github.com/datopian/markdowndb",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "dependencies": {
    "@flowershow/remark-wiki-link": "^1.1.2",
    "gray-matter": "^4.0.3",
    "knex": "^2.4.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "sqlite3": "^5.1.6",
    "unist-util-select": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8"
  }
}
